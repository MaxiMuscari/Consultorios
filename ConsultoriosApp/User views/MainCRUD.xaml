<UserControl x:Class="ConsultoriosApp.MainCRUD"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ConsultoriosApp"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <StackPanel VerticalAlignment="Center">
            <Button x:Name="btnPatients" Content="Pacientes" Height="29" Width="112" Margin="10" Click="btnPatients_Click"/>
            <Button x:Name="btnMedics" Content="Medicos" Height="29" Width="112" Margin="10" Click="btnMedics_Click"/>
            <Button x:Name="btnTurns" Content="Turnos" Height="29" Width="112" Margin="10" Click="btnTurns_Click"/>
        </StackPanel>
    </Grid>
</UserControl>
